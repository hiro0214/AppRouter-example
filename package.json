{
  "name": "approuter-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "typed-scss-modules": "typed-scss-modules 'src/**/*.module.scss' -w -n none -e default --implementation sass -p.@ resources/js -L error",
    "eslint": "eslint --fix --ext .ts,.tsx src",
    "stylelint": "stylelint 'src/**/*.{css,scss}' --fix",
    "prepare": "husky",
    "dev": "next dev & npm run typed-scss-modules",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "lint-staged": {
    "src/**/*.{css,scss}": [
      "npm run stylelint"
    ],
    "src/**/*.{ts,tsx}": [
      "npm run eslint"
    ]
  },
  "dependencies": {
    "next": "14.2.14",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "axios": "^1.7.7",
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "14.2.14",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "postcss-scss": "^4.0.9",
    "postcss-syntax": "^0.36.2",
    "prettier": "^3.3.3",
    "sass": "^1.79.4",
    "stylelint": "^16.9.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-recommended": "^14.0.1",
    "stylelint-config-standard": "^36.0.1",
    "typed-scss-modules": "^8.0.1",
    "typescript": "^5.5.4"
  }
}
