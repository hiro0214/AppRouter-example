{
  "name": "approuter-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "eslint": "eslint --fix --ext .ts,.tsx src",
    "stylelint": "stylelint 'src/**/*.{css,scss}' --fix",
    "prepare": "husky",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "lint-staged": {
    "src/**/*.{css,scss}": [
      "npm run stylelint"
